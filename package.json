{
  "name": "rivet-plugin-tuesday-crowd",
  "packageManager": "yarn@3.5.0",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/bundle.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && tsx bundle.ts",
    "dev": "run-p watch:tsc watch:esbuild:sync",
    "watch:tsc": "tsc -b -w --preserveWatchOutput",
    "watch:esbuild": "tsx bundle.ts --watch",
    "watch:esbuild:sync": "tsx bundle.ts --watch --sync",
    "publish": "yarn npm publish --access public"
  },
  "dependencies": {
    "@ironclad/rivet-core": "^1.18.0",
    "@orama/orama": "^2.0.18",
    "@orama/plugin-data-persistence": "^2.0.18"
  },
  "devDependencies": {
    "esbuild": "^0.19.2",
    "npm-run-all": "^4.1.5",
    "recursive-copy": "^2.0.14",
    "tsx": "^3.12.10",
    "typescript": "^5.2.2"
  },
  "volta": {
    "node": "20.6.1"
  },
  "rivet": {
    "skipInstall": true
  },
  "author": "Cornelius Donley <cornelius@donley.cx>",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cddigi/rivet-plugin-tuesday-crowd.git"
  },
  "homepage": "https://github.com/cddigi/rivet-plugin-tuesday-crowd"
}
